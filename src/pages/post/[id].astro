---
import '../styles/pages/[about, contact].scss'

import Layout from '../layouts/Layout.astro'

var pages = []
await fetch(`https://blogfeed.adarshrkumar.dev/getAllIds`)
    .then(response => response.json())
    .then(json => {
        json.forEach(function(p, i) {
            pages.push({params: {id: p}})
        })
    })


export function getStaticPaths() {
  return pages;
}

await fetch(`https://blogfeed.adarshrkumar.dev/getPost?p=`);
---
<Layout title="Blog Post">
</Layout>